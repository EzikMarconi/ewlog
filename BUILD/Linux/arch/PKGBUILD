_pkgname=ewlog
pkgname=$_pkgname
pkgver=1.4.2
pkgrel=1
pkgdesc="EWLog Desktop"
arch=("x86_64")
url="https://www.ewlog.app"
license=("custom")
install=ewlog.install

source=(
"https://debrepo.ewlog.app/pool/non-free/e/$_pkgname/${_pkgname}_${pkgver}_amd64.deb"
)

sha256sums=('263e9e091b194cfb9d830e63f69a4bbf0dad9ddb7d7b78a414cc7716867aa9aa')

package() {
  cd "${srcdir}"
  bsdtar -xf data.tar.xz -C "$pkgdir"
}
